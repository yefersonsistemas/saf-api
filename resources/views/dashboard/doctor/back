btnBack.onclick = function() {
        swal({
        title: "¡Alerta!",
        text: "¿Seguro que quiere anular la consulta?",
        type: "warning",
        showCancelButton: true,
        confirmButtonColor: '#3085d6',
        confirmButtonText: "¡Si, anular la consulta",
        closeOnConfirm: false
        allowOutsideClick:false,
        },
        function(){
        swal("Anulado!", "La cita ha sido anulada.", "success");
        });
        .then(function(){
                window.location.href = '{{ route('checkin.day') }}'
            })
    };